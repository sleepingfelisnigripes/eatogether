{
  "expo": {
    "name": "eatogether",
    "slug": "eatogether",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "androidStatusBar": {"barStyle": "light-content","backgroundColor": "#F48C06", "translucent": false},
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "FOREGROUND_SERVICE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECORD_AUDIO",
        "INTERNET",
        "CAMERA"
      ],
      "config":{
        "googleMaps": {"apiKey": "AIzaSyDN0K6GjKqEzw8FPdsaU80Iz5dOI4M0LZY"}
      },
      "package": "site.eatogether.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Let Eatogether access your photos to let you share them with your friends.",
          "cameraPermission": "Let Eatogether access your camera."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "054cfb11-acff-404d-a3ec-e7888acb4d5d"
      }
    }
  }
}
